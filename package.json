{
  "name": "snakecult-site",
  "version": "1.0.0",
  "description": "This repository contains the source code and content for the SnakeCult.net website, built with Hugo.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build:css": "npx tailwindcss -i assets/css/main.css -o assets/css/main.processed.css --minify --postcss",
    "build:css:production": "npx tailwindcss -i assets/css/main.css -o assets/css/main.processed.css --minify --postcss",
    "build": "npm run build:css:production && HUGO_SKIP_SASS=true hugo --minify",
    "test": "npm run build:css && HUGO_SKIP_SASS=true node scripts/verify_hugo_build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andcut/snakecult-site.git"
  },
  "keywords": [
    "hugo",
    "postcss"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andcut/snakecult-site/issues"
  },
  "homepage": "https://github.com/andcut/snakecult-site#readme",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.16",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17"
  }
}
